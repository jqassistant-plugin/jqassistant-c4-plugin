<jqassistant-plugin xmlns="http://schema.jqassistant.org/plugin/v2.4"
                    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                    xsi:schemaLocation="http://schema.jqassistant.org/plugin/v2.4 https://jqassistant.github.io/jqassistant/current/schema/jqassistant-plugin-v2.4.xsd"
                    name="jQAssistant C4 Plugin" id="jqa.plugin.c4"
                    version="${project.version}">
    <description>${project.description}</description>
    <model>
        <class>org.jqassistant.plugin.c4.model.BoundaryDescriptor</class>
        <class>org.jqassistant.plugin.c4.model.ComponentDescriptor</class>
        <class>org.jqassistant.plugin.c4.model.ContainerDescriptor</class>
        <class>org.jqassistant.plugin.c4.model.C4Descriptor</class>
        <class>org.jqassistant.plugin.c4.model.C4DiagramDescriptor</class>
        <class>org.jqassistant.plugin.c4.model.C4FileDescriptor</class>
        <class>org.jqassistant.plugin.c4.model.SystemDescriptor</class>
        <class>org.jqassistant.plugin.c4.model.PersonDescriptor</class>
    </model>
    <scanner>
        <class>org.jqassistant.plugin.c4.C4DiagramScannerPlugin</class>
    </scanner>
</jqassistant-plugin>